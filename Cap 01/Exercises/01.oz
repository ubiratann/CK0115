%item a:
declare Pow2 Pow4 Pow10 Pow50 Pow100
Pow2 = 2 * 2
Pow4 = Pow2 * Pow2
Pow10 = Pow4 * Pow4 * Pow2
Pow50 = Pow10 * Pow10 * Pow10 * Pow10* Pow10
Pow100 = Pow50 * Pow50
{Browse Pow100}

